{
    "name": "antalaron/appveyor-test",
    "description": "AppVeyor test",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Antal √Åron",
            "email": "antalaron@antalaron.hu"
        }
    ],
    "require": {
        "symfony/validator": "^3.2.0",
        "symfony/polyfill-mbstring": "^1.2"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "conflict": {
            "phpunit/phpunit": "<4.8.35|<5.4.3,>=5.0"
    },
    "autoload": {
        "psr-4": {
            "Antalaron\\Component\\AppVeyorTest\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Antalaron\\Component\\AppVeyorTest\\Tests\\": "tests/Tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
